@Model Escuela

@{
    ViewData["Title"] = "Program et Action";
    Layout = "Simple";
}

<div>
    <h1>School : @Model.UniqueId</h1>
    <h2>Name : @Model.Nombre</h2>
    <h3>Tipo de Escuela: @Model.TipoEscuela</h3>
    <address>
        <p>Dirección: @Model.Dirección </p>
        <p>País: @Model.Pais</p>
        <p>Ciudad: @Model.Ciudad</p>

    </address>
    <p> <strong>Año Fundación: @Model.AnoDeCreacion</strong> </p>

    <p><em> @ViewBag.Information </em></p>
</div>